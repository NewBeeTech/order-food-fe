.history {
  width: 750rpx;
  padding: 20rpx;
  background-color: #fff;
  cursor: pointer;
  display: flex;
  justify-content: center;
  align-items: center;
}
.history:not(:first-child) {
  margin-top: 20rpx;
}
.history-left {
  width: 80%;
}
.history-right {
  text-align: right;
  width: 10%;
  color: #bbb;
}
.history-first {
  display: flex;
  width: 100%;
  margin: 0 auto;
  color: #666666;
  font-size: 28rpx;
}
.history-second {
  display: flex;
  width: 100%;
  margin: 0 auto;
  font-size: 28rpx;
  color: #101010;
  font-weight: bold;
  margin-top: 10rpx;
  align-items: center;
  margin-bottom: 10rpx;
  /*justify-content: space-between;*/
}

.city {
  display: flex;
  align-items: center;
  width: 100%;
  margin: 0 auto;
  margin-top: 10rpx;
  color: #101010;
  font-size: 28rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
}
